/* pages/charging/charging.wxss */
page{
  width: 100%;
  min-height:100%;
}
.contact_top{
  width: 100%;
  margin-top:5%;
  font-size: 15px;
  text-align: center;
}
.biaoti{
  width: 100%;
  height: 120rpx;
  background: #000000;
  margin-top:10rpx;
  
  align-items: center;
}
.biaoti text{
  color: #f0f0f0;
  font-size: 13px;
}
.biaoti image{
  width: 80rpx;
  height: 80rpx;
}
.biaoti1{
  margin-left:10%;
}
.biaoti2{
   margin-left:40%;
}
.menu{
  padding-top: 10%;
  padding-left:20%;
}
.menu view{
  width: 200rpx;
  height: 80rpx;
  border: 1px;
  border-style: solid;
  
}
.menu text{
   font-size: 13px;
}
.op{
  display: flex;
  align-items: center;
  justify-content: center;
}
.option{
  margin-left:10%;
}
.xiangmu{
  margin-left:5%;
  border:1px;
  border-style: solid;
  border-radius: 100px;
  width: 120rpx;
  height: 120rpx;
  font-size:12px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.xiangmu_title{
  margin-top:5%;
  display: flex;
  align-items: center;
}
.xiangmu_title text{
  font-size:13px;
}
.xiangmu_title text:nth-child(2){
  margin-left: 10%;
}
.xiangmu_title text:nth-child(3){
  margin-left: 35%;
}